###  http://purl.obolibrary.org/obo/BFO_0000121
obo:BFO_0000121 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:BFO_0000117 ;
                owl:inverseOf obo:BFO_0000139 ;
                rdf:type owl:TransitiveProperty ;
