        <rdfs:subClassOf rdf:resource="https://w3id.org/lehrplan/ontology/LP_0000001"/>
        <rdfs:comment xml:lang="de">Ein “MINT-Fach” ist ein “Schulfach”, in dem sich die Lernenden mit einem der vier Bereiche Mathematik, Informatik, Naturwissenschaft und Technik auseinandersetzen.</rdfs:comment>
        <rdfs:label xml:lang="de">MINT-Fach</rdfs:label>
    </owl:Class>
    


